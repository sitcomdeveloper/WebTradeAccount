$(document).ready(function() {
    $(".form select, .form input[type=radio], .form input[type=checkbox]").uniform();
    $("label").each(function() {
        $(this).html($(this).html().replace("*", "<span>*<\/span>"));
        $(this).wrapInner("<span><\/span>")
    });
    $(".blank").each(function() {
        $(this).attr("target", "_blank")
    });
    $(".hp-cols .box").responsiveEqualHeightGrid();
    $("#nav > ul > li:last").addClass("last-child");
    updateDrops();
    initDropDown();
    initPopups();
    $(".table:not(.default-table) table").not(".account-table table").stacktable();
    $(".account-table:not(.default-table) table").stacktable2();
    initTabs(".tabset");
    $(".quote__container").initQuotes({
        updateInterval: 4e3,
        liMarqueOpts: {
            circular: !0,
            loop: -1,
            direction: "left",
            scrollamount: 50,
            hoverstop: !1
        }
    });
    $(".header-panel a.link").on("click", function() {
        var n = $(this).hasClass("active");
        return $(".header-panel .drop:visible").slideUp().prev().removeClass("active"), n ? ($(this).removeClass("active"), $(this).next().slideUp()) : ($(this).addClass("active"), $(this).next().slideDown()), !1
    });
    $(".lang-nav .drop a").on("click", function() {
        $("a.lng").html($(this).html());
        $(".lang-nav .drop").slideUp();
        $(".header-panel .link").hasClass("active") && $(".header-panel .link").removeClass("active")
    });
    $(".mb-lang-nav .drop a").on("click", function() {
        $("a.lng").html($(this).html());
        $(".mb-lang-nav .drop").slideUp();
        $(".mb-lang-nav .link").hasClass("active") && $(".mb-lang-nav .link").removeClass("active")
    });
    $(".mb-lang-nav a.link").on("click", function() {
        var n = $(this).hasClass("active");
        return n ? ($(this).removeClass("active"), $(this).next().slideUp()) : ($(this).addClass("active"), $(this).next().slideDown()), !1
    });
    $("body").on("click", function(n) {
        $(n.target).parents().hasClass("header-panel") || ($(".header-panel .link").hasClass("active") && $(".header-panel .link").removeClass("active"), $(".header-panel .drop").slideUp())
    });
    $.browser.msie && $.browser.version <= 9 && $("input:text, input:password, textarea").placeholder();
    $.browser.msie && $.browser.version < 9 && ($("body").addClass("lt9"), $(".table-list").each(function() {
        $(this).find("tr:even").addClass("colored")
    }));
    $.browser.msie && $.browser.version == 9 && $("body").addClass("ie9")
});
$(function(n) {
    n.fn.equalHeight = function() {
        var t = [];
        return n.each(this, function(i, r) {
            $element = n(r);
            var u, f = $element.css("box-sizing") == "border-box" || $element.css("-moz-box-sizing") == "border-box";
            u = f ? $element.outerHeight() : $element.height();
            t.push(u)
        }), this.css("height", Math.max.apply(window, t) + "px"), this
    };
    n.fn.equalHeightGrid = function(t) {
        var r = this,
            i, u, f;
        for (r.css("height", "auto"), i = 0; i < r.length; i++)
            if (i % t == 0) {
                for (u = n(r[i]), f = 1; f < t; f++) u = u.add(r[i + f]);
                u.equalHeight()
            } return this
    };
    n.fn.detectGridColumns = function() {
        var t = 0,
            i = 0;
        return this.each(function(r, u) {
            var f = n(u).offset().top;
            if (t === 0 || f == t) i++, t = f;
            else return !1
        }), i
    };
    n.fn.responsiveEqualHeightGrid = function() {
        function i() {
            var n = t.detectGridColumns();
            t.equalHeightGrid(n)
        }
        var t = this;
        return n(window).bind("resize load", i), i(), this
    };
    n.fn.stacktable = function(t) {
        var r = this,
            i = n.extend({}, {
                id: "mobile-table",
                hideOriginal: !0
            }, t);
        return r.each(function() {
            var u = n('<table class="' + i.id + '"><tbody><\/tbody><\/table>'),
                t, r;
            typeof i.myClass !== undefined && u.addClass(i.myClass);
            t = "";
            r = !0;
            $table = n(this);
            $table.addClass("desktop-table");
            $topRow = $table.find("tr").eq(0);
            $table.find("th").each(function() {
                var t = n(this).html();
                (t == " " || t == "&nbsp;") && n(this).html("")
            });
            $table.find("tr").each(function(i) {
                if (i !== 0) {
                    if (n("th, td", n(this)).length == 1 && n("th, td", n(this)).attr("colspan") && n("th, td", n(this)).attr("colspan") != "1") {
                        var u = n("th, td", n(this)).attr("colspan"),
                            f = n("th, td", n(this)).html();
                        n(this).find("td,th").each(function(i) {
                            r = i === 0 ? !r : r;
                            var e = r ? "odd" : "";
                            n(this).html() !== "" && (t += '<tr class="' + e + '">', $topRow.find("td,th").each(function(i) {
                                t += '<td class="st-key">' + n(this).html() + "<\/td>";
                                i === 0 && (t += '<td class="st-val" rowspan="' + u + '">' + f + "<\/td>");
                                t += "<\/tr>"
                            }))
                        });
                        return
                    }
                    n(this).find("td,th").each(function(i) {
                        var e, u, f;
                        r = i === 0 ? !r : r;
                        e = r ? "odd" : "";
                        n(this).html() !== "" && (t += '<tr class="' + e + '">', u = "colspan=2", f = "st-dbl", $topRow.find("td,th").eq(i).html() && (t += '<td class="st-key">' + $topRow.find("td,th").eq(i).html() + "<\/td>", u = "", f = ""), t += '<td class="st-val ' + f + '"' + u + ">" + n(this).html() + "<\/td>", t += "<\/tr>")
                    })
                }
            });
            u.append(n(t));
            $table.before(u);
            i.hideOriginal || $table.show()
        })
    };
    n.fn.stacktable2 = function(t) {
        var r = this,
            i = n.extend({}, {
                id: "mobile-table",
                hideOriginal: !0
            }, t);
        return r.each(function() {
            var u = n('<table class="' + i.id + '"><tbody><\/tbody><\/table>'),
                t, r;
            typeof i.myClass !== undefined && u.addClass(i.myClass);
            t = "";
            r = !0;
            $table = n(this);
            $table.addClass("desktop-table");
            $topRow = $table.find("tr").eq(0);
            $table.find("th").each(function() {
                var t = n(this).html();
                (t == " " || t == "&nbsp;") && n(this).html("")
            });
            $table.find("tr").each(function(i) {
                if (i !== 0) {
                    if (n("th, td", n(this)).length == 1 && n("th, td", n(this)).attr("colspan") && n("th, td", n(this)).attr("colspan") != "1") {
                        var u = n("th, td", n(this)).attr("colspan"),
                            f = n("th, td", n(this)).html();
                        n(this).find("td,th").each(function(i) {
                            r = i === 0 ? !r : r;
                            var e = r ? "odd" : "";
                            n(this).html() !== "" && (t += '<tr class="' + e + '">', $topRow.find("td,th").each(function(i) {
                                t += '<td class="st-key">' + n(this).html() + "<\/td>";
                                i === 0 && (t += '<td class="st-val" rowspan="' + u + '">' + f + "<\/td>");
                                t += "<\/tr>"
                            }))
                        });
                        return
                    }
                    n(this).find("td,th").each(function(i) {
                        var e, u, f;
                        r = i === 0 ? !r : r;
                        e = r ? "odd" : "";
                        i % 2 == 0 && n(this).html() !== "" && (t += '<tr class="' + e + '">', u = "colspan=2", f = "st-dbl", $topRow.find("td,th").eq(i).html() && (t += '<td class="st-key">' + $topRow.find("td,th").eq(i).html() + "<\/td>", t += $topRow.find("td,th").eq(i + 1).html() ? '<td class="st-key">' + $topRow.find("td,th").eq(i + 1).html() + "<\/td>" : '<td class="st-key"><\/td>', u = "", f = "", t += "<\/tr>"), t += "<tr>", t += '<td class="st-val ' + f + '"' + u + ">" + n(this).html() + "<\/td>", t += n(this).next().html() ? '<td class="st-val ' + f + '"' + u + ">" + n(this).next().html() + "<\/td>" : '<td class="st-val"><\/td>', t += "<\/tr>")
                    })
                }
            });
            u.append(n(t));
            $table.before(u);
            i.hideOriginal || $table.show()
        })
    }
}(jQuery));
$.fn.initQuotes = function(n) {
    return $(this).each(function() {
        function o(n, i) {
            i == 200 && s(t.responseText)
        }

      

        function s(t) {
            var e = $("<div><\/div>"),
                u = "";
            t = t.replace(/\s+(<)/ig, "<");
            e.html(t);
            u = e.find(".quotes").html();
            f ? (i.find(".str_origin, .str_move_clone").find(">.quote").remove(), i.find(".str_origin, .str_move_clone").prepend(u), r()) : (i.html(u).liMarquee(n.liMarqueOpts), r(), f = !0)
        }

        function r() {
            i.find(".str_move_clone").each(function() {
                $(this).width(i.find(".str_origin").outerWidth())
            })
        }
        var i = $(this),
            u = 4e3,
            f = !1,
            t;
        setInterval(e, u);
        e();
        $(window).on("load", function() {
            r()
        })
    })
}